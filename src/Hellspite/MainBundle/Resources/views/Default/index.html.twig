    <link rel="stylesheet" type="text/css" href="{{
    asset('bundles/hellspitemain/css/main.css') }}" />
{% endblock %}

{% block body %}
    <div id="container">
        <div id="header">
            {% block header %}
                <img src="{{ asset('bundles/hellspitemain/images/logo.svg') }}" alt="Hellspite" />
            {% endblock %}
        </div>
        <div id="content">

        </div>
    </div>
{% endblock %}
